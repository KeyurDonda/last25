                    pageNumber++;
                    var url = "";
                    url = new URL("http://localhost:9292/scans.js?filter_active=yours&filter_finished=yours&filter_suspended=yours&finished_page="+pageNumber)
                    window.location.replace(url.href);
                }

                function clickFin2() {
    
