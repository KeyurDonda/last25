<script>
        //Finished
                
                function clickFin1() {
    
                    var pageNumber = <%= @scanpagef.inspect%>
                    var lastPageNumber = <%= @newCountFin.inspect%>

                    if(pageNumber == lastPageNumber) {
                        var url = "";
                        url = new URL("http://localhost:9292/scans.js?filter_active=yours&filter_finished=yours&filter_suspended=yours&finished_page="+pageNumber)
                        return window.location.replace(url.href);
