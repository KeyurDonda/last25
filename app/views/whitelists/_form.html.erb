<%= simple_form_for(@whitelist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn' %>
    <%= link_to t('buttons.cancel'), url_for(:back), class: 'btn' %>
  </div>
<% end %>
